<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="party.infoo.entity">
    <class name="Manager" table="t_manager">
        <id name="managerId" column="managerId">
            <generator class="native"/>
        </id>
        <property name="managerName" column="managerName" type="java.lang.String" length="20"/>
        <property name="managerPass" column="managerPass" type="java.lang.String" length="20"/>
    </class>


</hibernate-mapping>
