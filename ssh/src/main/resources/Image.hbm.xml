<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="party.infoo.entity">
    <class name="Image" table="t_image">
        <id name="imageId" column="imageId">
            <generator class="native" />
        </id>
        <property name="imageName" column="imageName" type="java.lang.String" length="20"/>
        <property name="imageBlob" column="imageBlob" type="java.sql.Blob"/>
    </class>

</hibernate-mapping>
